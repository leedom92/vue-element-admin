{
  "name": "vue-element-admin",
  "version": "5.0.8",
  "packageManager": "pnpm@8.6.11",
  "description": "A magical vue admin. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
  "author": "Leedom",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "preview": "node build/index.js --preview",
    "lint:es": "eslint .",
    "lint:style": "stylelint ./src/**/*.{css,scss,vue}",
    "lint": "pnpm lint:es && pnpm lint:style",
    "lint:fix": "pnpm lint:es --fix && pnpm lint:style --fix",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "clipboard": "^2.0.11",
    "core-js": "^3.38.1",
    "element-ui": "^2.15.14",
    "fuse.js": "^3.6.1",
    "js-cookie": "^3.0.5",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.2",
    "screenfull": "4.2.1",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "7.24.8",
    "@leedomjs/eslint-config": "^0.11.3",
    "@leedomjs/stylelint-config": "^0.3.0",
    "@leedomjs/tailwindcss-preset": "^0.8.0",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "autoprefixer": "^10.4.20",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "body-parser": "^1.20.2",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "connect": "3.6.6",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.41",
    "runjs": "4.4.2",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "serve-static": "1.15.0",
    "stylelint": "^16.8.2",
    "svg-sprite-loader": "6.0.11",
    "svgo": "3.2.0",
    "tailwindcss": "^3.4.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/leedom92/vue-element-admin/issues"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js,vue}": "eslint --fix",
    "src/**/*.{css,scss,vue}": "stylelint --aei --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leedom92/vue-element-admin.git"
  }
}
